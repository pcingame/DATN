<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appchatdemo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_removebg_preview"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_removebg_preview"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppChatDemo">
        <activity
            android:name=".activities.CreateGroupChatActivity"
            android:exported="false" />
        <activity
            android:name=".activities.PrivateMessageActivity"
            android:exported="false" />
        <activity
            android:name=".activities.GroupMessageActivity"
            android:exported="false" />
        <activity
            android:name=".activities.CreatGroupChatActivity"
            android:exported="false" />
        <activity
            android:name=".activities.DashBoardActivity"
            android:exported="false" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>